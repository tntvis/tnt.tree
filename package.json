  },
  "licenses": [
    {
      "type": "Apache 2",
      "url": "https://github.com/emepyc/tnt.tree/blob/master/LICENSE"
    }
  ],
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha test/test.js",
    "build": "mkdirp build && scss lib/index.scss > build/index.css && browserify -r ./:tnt.tree -o build/tnt.tree.js",
    "build-browser": "npm run build",
    "prepublish": "npm run build",
    "watch": "watchify -r ./:tnttree -v -o build/tnt.tree.js",
    "sniper": "biojs-sniper ."
  },
  "dependencies": {
    "biojs-events": "0.0.4",
    "d3": "^3.5.3",
    "tnt.api": "0.0.0",
    "tnt.newick": "0.0.2",
    "tnt.tooltip": "0.0.4",
    "tnt.tree.node": "0.0.0",
    "tnt.utils": "0.0.0"
  },
  "devDependencies": {
    "mkdirp": "^0.5.0",
    "browserify": "6.x",
    "biojs-sniper": "0.x",
    "watchify": "^1.0.6",
    "chai": "1.x",
    "mocha": "1.x"
  },
  "sniper": {
    "js": [
      "/build/tnt.tree.js", "/web-components/ensembl-species-tree/pics.js"
    ],
    "css": [
      "/css/index.css"
    ],
    "snippets": [
      "examples"
    ],
    "first": "ensembl"
  },
  "keywords": [
    "phylogenetics",
    "tnt",
    "tree",
    "biojs"
  ]
}
